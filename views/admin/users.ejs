<table id="tblUsers" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <!-- <th>使用者編號</th> -->
            <th>帳號</th>
            <th>名稱</th>
            <th>電子信箱</th>
            <th>電話號碼</th>
            <th>角色</th>
        </tr>
    </thead>

    <tbody>
        <% users.forEach(function(user){ %>
          <tr>
            <!-- <td><%=user.M_ID %></td> -->
            <td><%=user.ACCOUNT %></td>
            <td><%=user.NAME %></td>
            <td><%=user.EMAIL %></td>
            <td><%=user.TELPHONE %></td>
            <td><%=user.R_NAME %></td>
          </tr>
        <%  }); %>
    </tbody>
</table>
<script type="text/javascript">
  $(function(){
    $("#tblUsers").DataTable();
  });

</script>
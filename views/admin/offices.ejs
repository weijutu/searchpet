<table id="tblOffices" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <!-- <th>使用者編號</th> -->
            <th>機關構名稱</th>
            <th>角色</th>
            <th>電話號碼</th>
            <th>地址</th>
            <th>信箱</th>
        </tr>
    </thead>
    <tbody>
        <% offices.forEach(function(o){ %>
          <tr>
            <td><%=o.NAME %></td>
            <td><%=o.R_NAME %></td>
            <td><%=o.TELPHONE %></td>
            <td><%=o.ADDRESS %></td>
            <td><%=o.EMAIL %></td>
          </tr>
        <%  }); %>
    </tbody>
</table>
<script type="text/javascript">
  $(function(){
    $("#tblOffices").DataTable( {
      "aLengthMenu": [[25, 50, 75, -1], [25, 50, 75, "All"]],
      "pageLength": 25
    });
  });

</script>
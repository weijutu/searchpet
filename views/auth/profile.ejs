<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ../partials/metadata.ejs %>
    <%- include ../partials/style.ejs %>
    <script src="/assets/vendor/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <link href="/assets/vendor/bootstrap-fileinput/css/fileinput.min.css" media="all" rel="stylesheet" type="text/css" />
   
    <title><%=title %></title>
</head>
<body>
    <%- include ../partials/navbar.ejs %>
    <div class="container">
    <!-- Page Header -->
        <div class="row">
            <div class="col-lg-12">
                <h2 class="page-header"><%=title %>
                    <small>更新資料</small>
                </h2>
            </div>
        </div>
        <div class="row">
            <form class="form-horizontal">
                <div class="form-group">
                    <label for="txtUserId" class="control-label col-xs-2">姓名</label>
                    <div class="col-xs-10">
                        <input type="text" class="form-control" id="txtUserId" placeholder="請輸入您的姓名">
                    </div>
                </div>
                <div class="form-group">
                    <label for="txtUserId" class="control-label col-xs-2">會員帳號</label>
                    <div class="col-xs-10">
                        <input type="text" class="form-control" id="txtUserId" placeholder="請輸入會員編號">
                    </div>
                </div>
                
                <div class="form-group">
                    <div class="col-xs-offset-2 col-xs-10">
                        <button type="submit" class="btn btn-primary">更新資料</button>
                    </div>
                </div>
            </form>
        </div>
        <!-- Footer -->
        <%- include ../partials/footer.ejs %>
    </div>
    <script src="/assets/vendor/js/jquery.js"></script>
    <script src="/assets/vendor/js/bootstrap.min.js"></script>
    
     
</body>

</html>